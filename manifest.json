{
  "manifest_version": 2,

  "name": "17TRACK",
  "description": "Track your packages with 17TRACK ALL-IN-ONE PACKAGE TRACKING",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "icons": {
    "128" : "128.png",
    "64" : "64.png",
    "32" : "32.png",
    "16" : "16.png"
  },

  "permissions": [
    "contextMenus"
  ]
}
